EMPLOYEE LIST
  url:http://127.0.0.1:8000/api/employees/
  method:get

EMPLOYEE CREATE
  url:http://127.0.0.1:8000/api/employees/
  method:post
  data:{name,dept,salary,address,phone,email}

EMPLOYEE DETAIL
  url:http://127.0.0.1:8000/api/employees/{id}/
  method:get
  
EMPLOYEE DELETE
  url:http://127.0.0.1:8000/api/employees/{id}/
  method:delete

EMPLOYEE UPDATE
  url:http://127.0.0.1:8000/api/employees/{id}/
  method:put
  data:{name,dept,salary,address,phone,email}
  
  
  
